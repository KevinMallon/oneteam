<h4>Days To Fill Request</h4>
<table id='days_to_fill' class="tablesorter table table-striped table-bordered requests report">
  <thead>
    <tr>
      <th>1 Day</th>
      <th>1-3_Days</th>
      <th>3-6_Days</th>
      <th>Over_6_Days</th>
    </tr>    
  </thead>
  <tbody>
    <% @dashboard.days_to_fill.each do |total| %>
      <tr>
        <td><%= total["1_Day"] %></td>
        <td><%= total["1-3_Days"] %></td>
        <td><%= total["3-6_Days"] %></td>
        <td><%= total["Over_6_Days"] %></td>
      </tr>
    <% end %>
  </tbody>
</table> 
<br>


